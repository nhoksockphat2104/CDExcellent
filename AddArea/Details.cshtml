@model CDExcellent.Models.AddArea

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutDashboardAdmin.cshtml";
}

<div class="container-md">
    <h2>Details Area</h2>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.AreaCode)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AreaCode)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.AreaName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AreaName)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DistributorQty)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DistributorQty)
        </dd>

    </dl>
    <a href="@Url.Action("Edit", "AddArea", new { id = Model.AreaId})" class="btn btn-sm btn-primary px-3">Edit</a>
    <a href="@Url.Action("Index", "AddArea")" class="btn btn-sm btn-dark px-3 m-3">Back</a>
</div>
